{
  "name": "silicon-html",
  "description": "Silicon - Multipurpose Bootstrap 5 Template & UI Kit",
  "version": "1.7.0",
  "type": "module",
  "homepage": "https://silicon.createx.studio",
  "author": "https://createx.studio/",
  "license": "ISC",
  "main": "index.html",
  "scripts": {
    "dev": "npm-run-all --silent -p styles scripts vendor && npm run watch --silent",
    "build": "npm-run-all --silent -p styles scripts vendor && npm run validate --silent",
    "styles": "npm-run-all -s styles:compile styles:minify",
    "styles:compile": "node build/styles.js compile",
    "styles:minify": "node build/styles.js minify",
    "scripts": "npm-run-all -s scripts:compile scripts:minify",
    "scripts:compile": "node build/scripts.js compile",
    "scripts:minify": "node build/scripts.js minify",
    "vendor": "node build/vendor.js",
    "validate": "node build/validate.js",
    "watch": "node build/watch.js"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.25.8",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "autoprefixer": "^10.4.20",
    "browser-sync": "^3.0.3",
    "cssnano": "^7.0.6",
    "eslint": "^9.13.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "fs-extra": "^11.2.0",
    "globals": "^15.11.0",
    "html-validate": "^8.24.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.47",
    "prettier": "^3.3.3",
    "rollup": "^4.24.0",
    "sass": "^1.80.3",
    "stylelint": "^16.10.0",
    "stylelint-config-twbs-bootstrap": "^15.1.0",
    "terser": "^5.36.0"
  },
  "dependencies": {
    "@lottiefiles/lottie-player": "^2.0.12",
    "bootstrap": "^5.3.5",
    "boxicons": "^2.1.4",
    "cleave.js": "^1.6.0",
    "imagesloaded": "^5.0.0",
    "img-comparison-slider": "^8.0.6",
    "jarallax": "^2.2.1",
    "lightgallery": "^2.8.3",
    "nouislider": "^15.8.1",
    "parallax-js": "^3.1.0",
    "prismjs": "^1.30.0",
    "rellax": "^1.12.1",
    "shufflejs": "^6.1.1",
    "smooth-scroll": "^16.1.3",
    "swiper": "^11.2.6",
    "vanilla-tilt": "^1.8.1"
  }
